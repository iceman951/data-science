    "import pandas as pd\n",
    "from numpy import nan as NA\n",
    "\n",
    "a = pd.Series([2,NA,4,5])\n",
    "b = pd.Series([6,7,8,NA])\n",
    "c = pd.Series([10,NA,12,13])\n",
    "\n",
    "# Fill NA Data\n",
    "data = pd.DataFrame({'A':a, 'B':b, 'C':c})\n",
    "print(data)\n",
    "dataMean = data.fillna(data.mean())\n",
    "print(dataMean)"
   ]
